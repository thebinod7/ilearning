<!-- CREATE COURSE CONTENT -->
<section id="create-course-section" class="create-course-section">
    <div class="container">
        <div class="row">
                <div class="col-md-3">
                    <div class="create-course-sidebar">
                        <ul class="list-bar">
                            <li class="active"><span class="count">1</span>Create Course</li>
                            <li><span class="count">2</span>Add Contents</li>
                            <li><span class="count">3</span>Publish Course</li>
                        </ul>
                        <div class="support">
                            <a href="#"><i class="icon md-camera"></i>See how it work short tutorial</a>
                            <a href="#"><i class="icon md-flash"></i>Instant Support</a>
                        </div>
                    </div>
                </div>

            <div class="col-md-9">
                <div class="create-course-content">

                    <!-- COURSE BANNER -->
                    <div class="course-banner create-item">
                        <div class="row">
                            <div class="col-md-3">
                                <h4>Course Banner</h4>
                            </div>
                            <div class="col-md-9">
                                <div class="image-info">
                                    <img src="/images/img-upload.jpg" alt="">
                                </div>
                                <div class="upload-recrop">
                                    <div class="upload-image up-file">
                                        <a href="#"><i class="icon md-upload"></i>Upload image</a>
                                        <input type="file">
                                    </div>
                                    <!-- <div class="recrop">
                                        <a href="#"><i class="icon md-crop"></i>Recrop</a>
                                    </div> -->
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- END / COURSE BANNER -->

                    <!-- PROMO VIDEO -->
                    <div class="promo-video create-item">
                        <div class="row">
                            <div class="col-md-3">
                                <h4>Course Title</h4>
                            </div>
                            <div class="col-md-9">
                                <div class="form-item">
                                    <input type="text" name="title" placeholder="Enter sweet and short course title">
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- END / PROMO VIDEO -->

                    <!-- DESCRIPTION -->
                    <div class="description create-item">
                        <div class="row">
                            <div class="col-md-3">
                                <h4>Description</h4>
                            </div>
                            <div class="col-md-9">
                                <div class="description-editor text-form-editor">
                                    <textarea name="description" placeholder="Discussion content"></textarea>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- END / DESCRIPTION -->

                    <!-- INSTRUCTORS -->
                    <div class="instructors create-item">
                        <div class="row">
                            <div class="col-md-3">
                                <h4>Instructors</h4>
                            </div>
                            <div class="col-md-9">
                              <select name="instructor" class="form-control" id="instructor">
                               <option>Suman KC</option>
                               <option>Madan Ghimire</option>
                               <option>John Doe</option>
                               <option>Steven Wilson</option>
                              </select>
                            </div>
                        </div>
                    </div>
                    <!-- END / INSTRUCTORS -->

                    <!-- Category -->
                    <div class="instructors create-item">
                        <div class="row">
                            <div class="col-md-3">
                                <h4>Category</h4>
                            </div>
                            <div class="col-md-9">
                              <select name="category" class="form-control" id="category">
                               <option>Soft Skills</option>
                               <option>History</option>
                               <option>Arts</option>
                               <option>GK</option>
                              </select>
                            </div>
                        </div>
                    </div>
                    <!-- END / Category -->

                    <!-- Start DATE -->
                    <div class="promo-video create-item">
                        <div class="row">
                            <div class="col-md-3">
                                <h4>Start Date</h4>
                            </div>
                            <div class="col-md-9">
                                <div class="form-item">
                                    <input name="startDate" type="text" placeholder="Course start date">
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- END Start DATE -->

                    <!-- End DATE -->
                    <div class="promo-video create-item">
                        <div class="row">
                            <div class="col-md-3">
                                <h4>End Date</h4>
                            </div>
                            <div class="col-md-9">
                                <div class="form-item">
                                    <input name="endDate" type="text" placeholder="Course end date">
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- END End DATE -->

                    <!-- Min Students -->
                    <div class="promo-video create-item">
                        <div class="row">
                            <div class="col-md-3">
                                <h4>Min Students</h4>
                            </div>
                            <div class="col-md-9">
                                <div class="form-item">
                                    <input name="minStudents" type="text" placeholder="5">
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- END min students -->

                    <!-- Max Students -->
                    <div class="promo-video create-item">
                        <div class="row">
                            <div class="col-md-3">
                                <h4>Max Students</h4>
                            </div>
                            <div class="col-md-9">
                                <div class="form-item">
                                    <input name="maxStudents" type="text" placeholder="15">
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- END max students -->

                    <!-- TUITION FREE -->
                    <div class="promo-video create-item">
                        <div class="row">
                            <div class="col-md-3">
                                <h4>Price</h4>
                            </div>
                            <div class="col-md-9">
                              <div class="form-item">
                                  <input name="price" type="text" placeholder="course price">
                              </div>
                            </div>
                        </div>
                    </div>
                    <!-- END / TUITION FREE -->

                    <!-- FOR LEVEL FROM -->
                    <div class="for-level-from create-item">
                        <div class="row">
                            <div class="col-md-3">
                                <h4>For level from</h4>
                            </div>
                            <div class="col-md-9">
                                <div class="form-item form-radio radio-style">
                                    <input type="radio" id="beginer" name="for-level-from">
                                    <label for="beginer">
                                        <i class="icon-radio"></i>
                                        Beginer
                                    </label>
                                </div>

                                <div class="form-item form-radio radio-style">

                                    <input type="radio" id="intermediate"  name="for-level-from">
                                    <label for="intermediate">
                                        <i class="icon-radio"></i>
                                        Intermediate
                                    </label>
                                </div>

                                <div class="form-item form-radio radio-style">

                                    <input type="radio" id="professional"  name="for-level-from">
                                    <label for="professional">
                                        <i class="icon-radio"></i>
                                        Professional
                                    </label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- END / FOR LEVEL FROM -->

                    <!-- TAG -->
                    <div class="tag-item create-item">
                        <div class="row">
                            <div class="col-md-3">
                                <h4>Tag</h4>
                            </div>
                            <div class="col-md-9">
                                <div class="form-item">
                                    <input type="text">
                                    <span>type your tool, separated by comma or space</span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- END / TAG -->

                    <div class="form-action">
                        <input type="submit" value="Submit" class="submit mc-btn-3 btn-style-1">
                    </div>

                </div>
            </div>
        </div>
    </div>
</section>
<!-- END / CREATE COURSE CONTENT -->
